{
  "name": "paname-consulting-app",
  "description": "Paname Consulting - Application web pour la présentation des services ainsi que l'entreprise et la gestion des rendez-vous, utilisateurs et procedures d'etudes à l'étranger , voyage d'affaires et de tourisme",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "vercel": "vercel --prod",
    "preview": "vite preview",
    "lint": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf node_modules dist .vite coverage",
    "reset": "pnpm run clean && pnpm install"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "date-fns": "4.1.0",
    "framer-motion": "^12.23.26",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "pnpm": "10.27.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet": "6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@rollup/rollup-win32-x64-msvc": "4.54.0",
    "@tailwindcss/postcss": "4.1.18",
    "@types/aos": "^3.0.7",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "6.1.11",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "lint-staged": "^16.2.7",
    "postcss": "8.5.6",
    "prettier": "^3.7.4",
    "rollup": "4.54.0",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwindcss": "4.1.18",
    "terser": "5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}