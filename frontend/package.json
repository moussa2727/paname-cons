    "lint": "eslint . --fix --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf node_modules dist .vite coverage",
    "reset": "pnpm run clean && pnpm install"
